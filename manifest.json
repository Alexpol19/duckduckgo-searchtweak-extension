{
  "name": "DuckDuckGo Search extension",
  "description": "Extension for manage search results and search on the DuckDuckGo website",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "ducduckgo.svg"
  },
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": [
        "https://duckduckgo.com/?*"
      ]
    }
  ]
}
